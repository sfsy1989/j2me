<configuration>
<properties>
  <!-- API version system properties -->

  <property Key="bluetooth.api.version"
      Value="1.1"
      Scope="system"
      Comment="The version of the Java APIs for Bluetooth wireless technology that is supported."/>

  <property Key="obex.api.version"
      Value="1.1"
      Scope="system"
      Comment="The version of the Java APIs for OBEX that is supported."/>

  <!-- OBEX internal properties -->

  <property Key="obex.packetLength.max"
      Value="4096"
      Scope="internal"
      Comment="The maximum size OBEX packet that the device can receive. Range: 255-65535"/>

  <!-- IrDA internal properties -->

  <property Key="com.sun.midp.io.j2me.irdaobex.DiscoveryTimeout"
      Value="10000"
      Scope="internal"
      Comment="Time in milliseconds discovery is attempted before reporting failure."/>

  <property Key="com.sun.midp.io.j2me.irdaobex.DiscoveryInterval"
      Value="200"
      Scope="internal"
      Comment="Time in milliseconds between discovery attempts."/>

</properties>
</configuration>
